{
  "name": "pubg-tournament",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "devfront": "ng serve --proxy-config proxy.conf.json --open",
    "devfrontmock": "ng serve --proxy-config proxy.mock.conf.json --open --no-live-reload",
    "mock": "npm run mock -w back",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:verify": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:staged": "pretty-quick --staged",
    "precommit": "npm run lint:fix && pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@angular/service-worker": "~12.2.4",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fullcalendar/core": "^4.1.0",
    "@fullcalendar/daygrid": "^4.1.0",
    "@fullcalendar/interaction": "^4.1.0",
    "@fullcalendar/list": "^4.1.0",
    "@fullcalendar/moment": "^4.1.0",
    "@fullcalendar/timegrid": "^4.1.0",
    "@ng-dynamic-forms/core": "^14.0.1",
    "@ng-dynamic-forms/ui-primeng": "^14.0.1",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/schematics": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "chart.js": "^3.5.1",
    "core-js": "^3.17.2",
    "font-awesome": "^4.7.0",
    "moment": "^2.29.1",
    "ngx-countdown": "^12.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-moment": "^5.0.0",
    "primeflex": "^3.0.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.0",
    "rxjs": "~7.3.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@types/jasmine": "~3.9.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "~4.3.5"
  },
  "workspaces": [
    "./",
    "./back/"
  ]
}
