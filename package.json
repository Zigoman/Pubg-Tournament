{
  "name": "pubg-mobile-tournament",
  "version": "0.1.0",
  "author": "Nir Zigdon",
  "description": "Pubg Mobile group of friends, project built using Angular 10, Express, Mongoose and Node.",
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.9"
  },
  "scripts": {
    "ng": "ng",
    "start": "node server",
    "devfront": "ng serve --proxy-config proxy.conf.json --open",
    "serverbuild": "concurrently -c \"yellow.bold,green.bold\" -n \"SERVER,BUILD\" \"ng build --watch\" \"nodemon server\"",
    "devfrontmock": "ng serve --proxy-config proxy.mock.conf.json --open --no-live-reload",
    "mock": "json-server -p 5000 --watch mock/db.json --middlewares mock/middlewares.ts",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "ng lint --fix && pretty-quick --staged",
    "icons": "node webfontGenerator.js && webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "private": true,
